{
  "name": "whats-new",
  "description": "what's new in node",
  "repository": {
    "type": "git",
    "url": "git://github.com/gbgnodejs/whats-new"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "version": "0.0.0",
  "scripts": {
    "pack": "rm -rf whats-new.app && npm install && packify index.html > launch.html && node add_slides",
    "build": "npm run pack && atom-shell-packager . whats-new --ignore=node_modules --ignore=node_modules/atom-shell",
    "app": "npm run pack && open launch.html",
    "postbuild": "rm launch.html"
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "atom-shell": "^0.22.3",
    "browserify": "^4.1.11",
    "domify": "^1.2.2",
    "glob": "^5.0.3",
    "packify": "^1.1.2"
  },
  "devDependencies": {
    "atom-shell-packager": "^2.1.1"
  }
}
